Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyErricsson, Motorola web design" />
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <!--webfont-->
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800' rel='stylesheet' type='text/css'>

    <link href="~/web2/css/jqvmap.css" media="screen" rel="stylesheet" type="text/css" />


    <script src="~/web2/js/easyResponsiveTabs.js" type="text/javascript"></script>

    <script src="~/web2/js/main.js"></script> <!-- Resource jQuery -->
    <!-- chart -->
    <script src="~/web2/js/Chart1.js"></script>
    <!-- //chart -->
</head>
<body>
    <div class="total-content">
        <div class="list_of_members">
            <div class="sales" style="background-color:#82589F">
                <div class="icon-text">
                    <h3>@ViewBag.d1</h3>
                    <p>Total Company</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="new-users" style="background-color:#2ecc71">

                <div class="icon-text">
                    <h3>@ViewBag.d2</h3>
                    <p>Total Category</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="orders" style="background-color:#3498db">

                <div class="icon-text">
                    <h3>@ViewBag.d3</h3>
                    <p>Building Count</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="visitors" style="background-color:#9b59b6">

                <div class="icon-text">
                    <h3>@ViewBag.d4 </h3>
                    <p>Textile Count</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="clearfix"></div>
        </div>
    </div>
    <div class="clearfix"></div>



    <div class="total-content">
        <div class="list_of_members">
            <div class="sales" style="background-color:#D6A2E8">
                <div class="icon-text">
                    <h3>@ViewBag.d5</h3>
                    <p>Sum Giro</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="new-users" style="background-color:#58B19F">

                <div class="icon-text">
                    <h3>@ViewBag.d6 </h3>
                    <p>Technology Count </p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="orders" style="background-color:#82ccdd">

                <div class="icon-text">
                    <h3>@ViewBag.d7 </h3>
                    <p>Max Giro Company</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="visitors" style="background-color:#ffda79">

                <div class="icon-text">
                    <h3>@ViewBag.d8</h3>
                    <p>Min Giro Company</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="clearfix"></div>
        </div>
    </div>

    <div class="clearfix"></div>
    <div class="total-content">
        <div class="list_of_members">
            <div class="sales" style="background-color: #f78fb3">
                <div class="icon-text">
                    <h3>@ViewBag.d9 ₺</h3>
                    <p>Average Giro</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="new-users" style="background-color: #079992 ">

                <div class="icon-text">
                    <h3>@ViewBag.d10</h3>
                    <p>Food Company Giro</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="new-users" style="background-color: #B0A695 ">

                <div class="icon-text">
                    <h3>@ViewBag.d11</h3>
                    <p>Technology Company Giro</p>
                </div>
                <div class="clearfix"></div>
            </div>
            
        </div>
    </div>


    

